{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off",
        "document-title": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "button-type": "off",
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "css-prefix-order": "off",
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-required-parent": "off",
        "aria-valid-attr-value": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  }
}